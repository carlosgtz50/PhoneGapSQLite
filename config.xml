

<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonecdv.informegobiernocuu" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Informe de Gobierno CUU</name>
    <description>
        Regiones Competitivas S.C. - Gobierno del Estado de Chihuahua
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <preference name="permissions" value="none"/>
	  <preference name="orientation" value="default"/>
	  <preference name="target-device" value="universal"/>
	  <preference name="fullscreen" value="true"/>
	  <preference name="webviewbounce" value="true"/>
	  <preference name="prerendered-icon" value="true"/>
	  <preference name="stay-in-webview" value="false"/>
	  <preference name="ios-statusbarstyle" value="black-opaque"/>
	  <preference name="detect-data-types" value="true"/>
	  <preference name="exit-on-suspend" value="false"/>
	  <preference name="show-splash-screen-spinner" value="true"/>
	  <preference name="auto-hide-splash-screen" value="true"/>
	  <preference name="disable-cursor" value="false"/>
	  <preference name="android-minSdkVersion" value="14"/>
	  <preference name="android-installLocation" value="auto"/>
	  <icon src="icon.png"/>
	  <icon src="www/res/icon/android/icon-36-ldpi.png" cdv:platform="android" cdv:qualifier="ldpi"/>
	  <icon src="www/res/icon/android/icon-48-mdpi.png" cdv:platform="android" cdv:qualifier="mdpi"/>
	  <icon src="www/res/icon/android/icon-72-hdpi.png" cdv:platform="android" cdv:qualifier="hdpi"/>
	  <icon src="www/res/icon/android/icon-96-xhdpi.png" cdv:platform="android" cdv:qualifier="xhdpi"/>
	  <icon src="www/res/icon/blackberry/icon-80.png" cdv:platform="blackberry"/>
	  <icon src="www/res/icon/blackberry/icon-80.png" cdv:platform="blackberry" cdv:state="hover"/>
	  <icon src="www/res/icon/ios/icon-57.png" cdv:platform="ios" width="57" height="57"/>
	  <icon src="www/res/icon/ios/icon-72.png" cdv:platform="ios" width="72" height="72"/>
	  <icon src="www/res/icon/ios/icon-57-2x.png" cdv:platform="ios" width="114" height="114"/>
	  <icon src="www/res/icon/ios/icon-72-2x.png" cdv:platform="ios" width="144" height="144"/>
	  <icon src="www/res/icon/webos/icon-64.png" cdv:platform="webos"/>
	  <icon src="www/res/icon/windows-phone/icon-48.png" cdv:platform="winphone"/>
	  <icon src="www/res/icon/windows-phone/icon-173-tile.png" cdv:platform="winphone" cdv:role="background"/>
	  
      <access origin="*" />
</widget>
