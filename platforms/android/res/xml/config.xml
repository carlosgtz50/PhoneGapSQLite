<?xml version='1.0' encoding='utf-8'?>
<widget id="com.regionescompetitivas" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <name>Informe de Gobierno</name>
    <description>
        Regiones Competitivas S.C. - Gobierno del Estado de Chihuahua.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <icon src="icon.png" />
    <icon cdv:platform="android" cdv:qualifier="ldpi" src="www/res/icon/android/icon-36-ldpi.png" />
    <icon cdv:platform="android" cdv:qualifier="mdpi" src="www/res/icon/android/icon-48-mdpi.png" />
    <icon cdv:platform="android" cdv:qualifier="hdpi" src="www/res/icon/android/icon-72-hdpi.png" />
    <icon cdv:platform="android" cdv:qualifier="xhdpi" src="www/res/icon/android/icon-96-xhdpi.png" />
    <cdv:splash cdv:platform="android" cdv:qualifier="port-ldpi" src="www/res/screen/android/screen-ldpi-portrait.png" />
    <cdv:splash cdv:platform="android" cdv:qualifier="port-mdpi" src="www/res/screen/android/screen-mdpi-portrait.png" />
    <cdv:splash cdv:platform="android" cdv:qualifier="port-hdpi" src="www/res/screen/android/screen-hdpi-portrait.png" />
    <cdv:splash cdv:platform="android" cdv:qualifier="port-xhdpi" src="www/res/screen/android/screen-xhdpi-portrait.png" />
    <content src="index.html" />
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
</widget>
